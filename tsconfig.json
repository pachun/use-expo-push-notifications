    "strict": true,
    "noEmit": false,
    "outDir": "lib",
    "declaration": true,
    "module": "CommonJS",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },

  "include": ["src/**/*"]
}
